    <xs:element name="getMVDRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="passport" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="getMVDResponse">
